		<symbol symbol="star" x="ch/2-ch*x/7.5" y="ch/2-ch*x/7.5" sx="ch/20" sy="ch/20" color="white"/>
		<symbol symbol="star" x="ch/2+ch*x/7.5" y="ch/2-ch*x/7.5" sx="ch/20" sy="ch/20" color="white"/>
		<symbol symbol="star" x="ch/2-ch*x/7.5" y="ch/2+ch*x/7.5" sx="ch/20" sy="ch/20" color="white"/>
		<symbol symbol="star" x="ch/2+ch*x/7.5" y="ch/2+ch*x/7.5" sx="ch/20" sy="ch/20" color="white"/>
	</for>
</flag>