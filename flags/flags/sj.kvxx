<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE flag PUBLIC "-//Kreative//DTD Vexillo 1.0//EN" "http://www.kreativekorp.com/dtd/kvxx.dtd">
<flag id="sj" name="Svalbard and Jan Mayen" fly="h*11/8">
	<!-- Same as Norway. -->
	<defs>
		<dim id="y1" e="h*6/16"/>
		<dim id="y2" e="h*7/16"/>
		<dim id="y3" e="h*9/16"/>
		<dim id="y4" e="h*10/16"/>
		<dim id="x1" e="min(h*6/16,w/2-h/8)"/>
		<dim id="x2" e="min(h*7/16,w/2-h/16)"/>
		<dim id="x3" e="min(h*9/16,w/2+h/16)"/>
		<dim id="x4" e="min(h*10/16,w/2+h/8)"/>
		<color id="red">
			<colorspec model="pantone" value="032 U"/>
			<colorspec model="rgb" value="237 41 57"/>
		</color>
		<color id="blue">
			<colorspec model="pantone" value="281 U"/>
			<colorspec model="rgb" value="0 38 100"/>
		</color>
		<color id="white">
			<colorspec model="pantone" value="Safe"/>
			<colorspec model="rgb" value="255 255 255"/>
		</color>
	</defs>
	<field x1="0" y1="0" x2="w" y2="h" color="red"/>
	<cross x1="0" y1="0" x2="x1" y2="y1" x3="x4" y3="y4" x4="w" y4="h" color="white"/>
	<cross x1="0" y1="0" x2="x2" y2="y2" x3="x3" y3="y3" x4="w" y4="h" color="blue"/>
</flag>